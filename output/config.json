{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/en-US\\/animes\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/en-US\\/api\\/waitlist\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/en-US\\/newsletter\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/es-AR\\/animes\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/es-AR\\/api\\/waitlist\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/es-AR\\/newsletter\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/pt-BR\\/animes\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/pt-BR\\/api\\/waitlist\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/pt-BR\\/newsletter\\/?$",
			"dest": "_render"
		}
	]
}